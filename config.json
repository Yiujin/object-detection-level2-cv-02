{
  "name": "Fast-Rcnn",
  "wandb": {
    "env_path": "./.env",
    "entity": "boostcamp-2th-cv-02team",
    "project": "object-detection-recycling-trash",
    "unique_tag": ""
  },
  "n_gpu": 1,
  "arch": {
    "type": "FasterRCNN_ResNet50_FPN",
    "args": {}
  },
  "data_loader": {
    "type": "CustomDataset",
    "args": {
      "annotation": "../dataset/train.json",
      "data_dir": "../dataset",
      "transforms": "get_train_transform"
    }
  },
  "optimizer": {
    "type": "Adam",
    "args": {
      "lr": 0.001,
      "weight_decay": 0,
      "amsgrad": true
    }
  },
  "loss": "nll_loss",
  "metrics": ["accuracy", "top_k_acc"],
  "lr_scheduler": {
    "type": "StepLR",
    "args": {
      "step_size": 50,
      "gamma": 0.1
    }
  },
  "trainer": {
    "epochs": 2,
    "save_dir": "saved/",
    "save_period": 1,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 10,
    "tensorboard": true
  }
}
